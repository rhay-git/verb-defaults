{
  "name": "verb-defaults",
  "description": "Plugin for setting basic defaults in verb projects.",
  "version": "0.2.1",
  "homepage": "https://github.com/verbose/verb-defaults",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-defaults",
  "bugs": {
    "url": "https://github.com/verbose/verb-defaults/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "common-middleware": "^0.3.0",
    "engine-base": "^0.1.2",
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "verb": "github:verbose/verb#dev"
  },
  "keywords": [
    "defaults",
    "docs",
    "documentation",
    "generate",
    "generator",
    "plugin",
    "template",
    "templates",
    "verb",
    "verbplugin"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "verb",
        "verb-collections",
        "verb-repo-data"
      ]
    },
    "reflinks": [
      "engine-base",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
